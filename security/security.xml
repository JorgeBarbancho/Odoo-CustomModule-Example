<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <record model="ir.module.category" id="module_category_bookdatabase">
            <field name="name">Bookdatabase</field>
            <field name="description">Helps you manage your book database</field>
            <field name="sequence">1020</field>
        </record>

        <record model="res.groups" id="group_bookdatabase_user">
            <field name="name">Bookdatabase user</field>
            <field name="category_id" ref="module_category_bookdatabase"/>
        </record>

        <record model="res.groups" id="group_bookdatabase_manager">
            <field name="name">Bookdatabase manager</field>
            <field name="category_id" ref="module_category_bookdatabase"/>
            <field name="implied_ids" eval="[(4, ref('group_bookdatabase_user'))]"/>
            <field name="users" eval="[(4, ref('base.user_admin'))]"/>
        </record>

    </data>
</odoo>